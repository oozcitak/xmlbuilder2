{
  "compilerOptions": {
        "lib": [ "es2023" ],
        "module": "preserve",
        "target": "es2022",

        "strict": true,
        "declaration": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "newLine": "lf",
        "sourceMap": true,
        "outDir": "lib",
        "types": [ "node", "dedent" ]
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}